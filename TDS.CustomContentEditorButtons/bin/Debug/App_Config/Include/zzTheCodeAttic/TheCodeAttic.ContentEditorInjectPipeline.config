<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <pipelines>
      <renderContentEditor>
        <processor patch:before="*[1]" type="TheCodeAttic.CustomContentEditorButtons.ContentEditorInjectScripts, TheCodeAttic.CustomContentEditorButtons" />
      </renderContentEditor>
    </pipelines>
    <settings>
      <setting name="CustomContentEditorJavascript" value="/sitecore modules/Shell/TheCodeAttic/TheCodeAttic.CollapseExpandCommands.js"/>
      <!--<setting name="CustomContentEditorStylesheets" value="/path-to/style1.css|/path-to/style2.css|/path-to/style3.css"/>-->
    </settings>
  </sitecore>
</configuration>